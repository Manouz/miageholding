<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nguessanabigail.adjeaude.miageholding.Controller.adminlogistiqueController">
   <children>
      <BorderPane layoutX="0.0" layoutY="0.0" prefHeight="600.0" prefWidth="900.0">
         <top>
            <Pane fx:id="navbar" prefHeight="87.0" prefWidth="900.0" style="-fx-background-color: #47A1A4;">
               <children>
                  <HBox alignment="CENTER_LEFT" layoutX="6.0" layoutY="4.0" prefHeight="35.0" prefWidth="872.0">
                     <children>
                        <ImageView fitHeight="79.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../images/logogauche.png" />
                           </image>
                           <HBox.margin>
                              <Insets left="3.0" right="130.0" />
                           </HBox.margin>
                        </ImageView>
                        <Label text="GESTIONNAIRE DU PARC " textFill="#3c3939">
                           <font>
                              <Font size="30.0" />
                           </font>
                        </Label>
                        <Label text="AUTO" textFill="#e41212">
                           <font>
                              <Font name="System Bold" size="32.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </Pane>
         </top>
         <center>
            <AnchorPane fx:id="body" prefHeight="512.0" prefWidth="700.0" style="-fx-background-color: #F5F5F5;">
               <children>
                  <Label fx:id="welcomeLabel" layoutX="350.0" layoutY="248.0" text="Label" />
               </children></AnchorPane>
         </center>
         <left>
            <Pane fx:id="sidebar" prefHeight="430.0" prefWidth="185.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox prefHeight="513.0" prefWidth="185.0" spacing="5.0" style="-fx-background-color: Grey;">
                     <children>
                        <Button alignment="BASELINE_LEFT" contentDisplay="TEXT_ONLY" mnemonicParsing="false" onAction="#loadAccueilView" prefHeight="30.0" prefWidth="200.0" style="-fx-effect: #;" text="Tableau de Bord">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                        <Button alignment="BASELINE_LEFT" contentDisplay="TEXT_ONLY" mnemonicParsing="false" onAction="#loadAdministratifView" prefHeight="30.0" prefWidth="200.0" text="Parc Automobile">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                        <Button alignment="BASELINE_LEFT" contentDisplay="TEXT_ONLY" mnemonicParsing="false" onAction="#loadMissionsView" prefHeight="30.0" prefWidth="200.0" text="Missions">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                        <Button alignment="BASELINE_LEFT" contentDisplay="TEXT_ONLY" mnemonicParsing="false" onAction="#loadSTechniqueView" prefHeight="30.0" prefWidth="200.0" text="Suivie Technique">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                        <Button alignment="BASELINE_LEFT" contentDisplay="TEXT_ONLY" mnemonicParsing="false" onAction="#loadAffectationsView" prefHeight="30.0" prefWidth="200.0" text="Affectations">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                        <Button alignment="BASELINE_LEFT" contentDisplay="TEXT_ONLY" mnemonicParsing="false" onAction="#loadFinancementView" prefHeight="31.0" prefWidth="171.0" text="Financement Véhicule">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                        <Button alignment="BASELINE_LEFT" contentDisplay="TEXT_ONLY" mnemonicParsing="false" onAction="#loadInventaireView" prefHeight="30.0" prefWidth="200.0" text="Inventaire">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                        <Button alignment="BASELINE_LEFT" contentDisplay="TEXT_ONLY" mnemonicParsing="false" onAction="#loadStatView" prefHeight="30.0" prefWidth="200.0" text="Statistique &amp; Etats">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                        <Button alignment="BASELINE_LEFT" contentDisplay="TEXT_ONLY" mnemonicParsing="false" onAction="#loadSocietaireView" prefHeight="30.0" prefWidth="200.0" text="Sociétaire">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                        <Button alignment="BASELINE_LEFT" contentDisplay="TEXT_ONLY" mnemonicParsing="false" onAction="#loadParametresView" prefHeight="30.0" prefWidth="200.0" text="Parametres">
                           <font>
                              <Font size="15.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="40.0" />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logoutButtonOnAction" prefHeight="25.0" prefWidth="145.0" text="Déconnexion" textFill="#51acc3">
                           <style>
                              <String fx:value="-fx-background-color: transparent;" />
                           </style>
                        </Button>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="50.0" />
                     </padding>
                  </VBox>
               </children>
            </Pane>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
